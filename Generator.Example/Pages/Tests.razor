@page "/Tests"

<PageTitle>Counter</PageTitle>

<h1>Tests</h1>



<MudItem xs="12" sm="12" md="3" lg="6" xl="6" xxl="6">
    <button class="btn btn-primary m-2" @onclick="ParametricQuery">@("ParametricQuery".Humanize())</button>
</MudItem>

<MudItem xs="12" sm="12" md="3" lg="6" xl="6" xxl="6">
